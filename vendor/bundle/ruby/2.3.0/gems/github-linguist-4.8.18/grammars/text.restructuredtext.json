        },
        {
          "name": "meta.substitution-definition.restructuredtext",
          "begin": "^(\\s*)(\\.\\.)\\s(\\|)(?=\\S)([^\\|]+)((?<=\\S)\\|)\\s+(\\S+.*(?=::))(::)(.*)",
          "end": "^(?!\\s*$|\\1[ \\t]+\\S)",
          "patterns": [
