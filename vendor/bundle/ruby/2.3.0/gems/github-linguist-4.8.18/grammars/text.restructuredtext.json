      "patterns": [
        {
          "name": "meta.substitution-definition.image.restructuredtext",
          "begin": "^(\\s*)(\\.\\.)\\s(\\|)(?=\\S)([^\\|]+)((?<=\\S)\\|)\\s+(image)(::)\\s*(\\S+)",
